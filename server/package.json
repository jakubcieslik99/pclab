{
  "name": "pclab-server",
  "version": "1.5.1",
  "description": "PCLab",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "docker": "cross-env DIR=pclab-server docker-compose -f docker-compose.dev.yml -p pclab-server up --force-recreate",
    "dev": "cross-env ENV=development nodemon --watch src --exec babel-node src/index.js",
    "build": "babel src -d dist",
    "lint": "eslint . --ignore-path ../.gitignore --ext .js",
    "format": "prettier --ignore-path ../.gitignore --write \"**/*.+(js)\"",
    "update:all": "corepack up && pnpm update"
  },
  "author": "Jakub Cie≈õlik",
  "license": "UNLICENSED",
  "packageManager": "pnpm@10.6.5+sha512.cdf928fca20832cd59ec53826492b7dc25dc524d4370b6b4adbf65803d32efaa6c1c88147c0ae4e8d579a6c9eec715757b50d4fa35eea179d868eada4ed043af",
  "dependencies": {
    "@infisical/sdk": "^3.0.7",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-fileupload": "^1.5.1",
    "express-rate-limit": "^6.11.2",
    "express-slow-down": "^1.6.0",
    "helmet": "^6.2.0",
    "http-errors": "^2.0.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^6.13.8",
    "node-schedule": "^2.1.1",
    "nodemailer": "^6.10.0",
    "pino": "^8.21.0",
    "pino-pretty": "^9.4.1",
    "stripe": "^11.18.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.10",
    "@babel/node": "^7.26.0",
    "@babel/preset-env": "^7.26.9",
    "cross-env": "^7.0.3",
    "eslint": "8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3"
  }
}
