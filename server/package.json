{
  "name": "pclab-server",
  "version": "1.8.0",
  "description": "PCLab",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "docker": "cross-env DIR=pclab-server docker-compose -f docker-compose.dev.yml -p pclab-server up --force-recreate",
    "dev": "cross-env ENV=development nodemon --watch src --exec babel-node src/index.js",
    "build": "babel src -d dist",
    "lint": "eslint .",
    "format": "prettier --ignore-path ../.gitignore --write \"**/*.+(js)\"",
    "update:all": "corepack up && pnpm update"
  },
  "author": "Jakub Cie≈õlik",
  "license": "UNLICENSED",
  "packageManager": "pnpm@10.11.1+sha512.e519b9f7639869dc8d5c3c5dfef73b3f091094b0a006d7317353c72b124e80e1afd429732e28705ad6bfa1ee879c1fce46c128ccebd3192101f43dd67c667912",
  "dependencies": {
    "@infisical/sdk": "^4.0.4",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-fileupload": "^1.5.2",
    "express-rate-limit": "^8.1.0",
    "express-slow-down": "^3.0.0",
    "helmet": "^8.1.0",
    "http-errors": "^2.0.0",
