{
  "name": "pclab-server",
  "version": "1.4.6",
  "description": "PCLab",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "docker": "cross-env DIR=pclab-server docker-compose -f docker-compose.dev.yml -p pclab-server up --force-recreate",
    "dev": "cross-env ENV=development nodemon --watch src --exec babel-node src/index.js",
    "build": "babel src -d dist",
    "lint": "eslint . --ignore-path ../.gitignore --ext .js",
    "format": "prettier --ignore-path ../.gitignore --write \"**/*.+(js)\"",
    "update:all": "corepack up && pnpm update"
  },
  "author": "Jakub Cie≈õlik",
  "license": "UNLICENSED",
  "packageManager": "pnpm@9.14.2+sha512.6e2baf77d06b9362294152c851c4f278ede37ab1eba3a55fda317a4a17b209f4dbb973fb250a77abc463a341fcb1f17f17cfa24091c4eb319cda0d9b84278387",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-fileupload": "^1.5.1",
    "express-rate-limit": "^6.11.2",
    "express-slow-down": "^1.6.0",
    "helmet": "^6.2.0",
    "http-errors": "^2.0.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^6.13.5",
    "node-schedule": "^2.1.1",
    "nodemailer": "^6.9.16",
    "pino": "^8.21.0",
    "pino-pretty": "^9.4.1",
    "stripe": "^11.18.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.0",
    "@babel/node": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "cross-env": "^7.0.3",
    "eslint": "8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2"
  }
}
