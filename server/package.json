{
  "name": "pclab-server",
  "version": "1.4.3",
  "description": "PCLab",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "docker": "cross-env DIR=pclab-server docker-compose -f docker-compose.dev.yml -p pclab-server up --force-recreate",
    "dev": "cross-env ENV=development nodemon --watch src --exec babel-node src/index.js",
    "build": "babel src -d dist",
    "lint": "eslint . --ignore-path ../.gitignore --ext .js",
    "format": "prettier --ignore-path ../.gitignore --write \"**/*.+(js)\"",
    "update:all": "corepack up && pnpm update"
  },
  "author": "Jakub Cie≈õlik",
  "license": "UNLICENSED",
  "packageManager": "pnpm@9.11.0+sha512.0a203ffaed5a3f63242cd064c8fb5892366c103e328079318f78062f24ea8c9d50bc6a47aa3567cabefd824d170e78fa2745ed1f16b132e16436146b7688f19b",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-fileupload": "^1.5.1",
    "express-rate-limit": "^6.11.2",
    "express-slow-down": "^1.6.0",
    "helmet": "^6.2.0",
    "http-errors": "^2.0.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^6.13.3",
    "node-schedule": "^2.1.1",
    "nodemailer": "^6.9.15",
    "pino": "^8.21.0",
    "pino-pretty": "^9.4.1",
    "stripe": "^11.18.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.7",
    "@babel/core": "^7.25.7",
    "@babel/node": "^7.25.7",
    "@babel/preset-env": "^7.25.7",
    "cross-env": "^7.0.3",
    "eslint": "8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3"
  }
}
